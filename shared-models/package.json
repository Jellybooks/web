{
  "name": "@jellybooks/shared-test",
  "version": "1.0.5",
  "description": "Next generation SDK for publications in Web Apps",
  "author": "Jellybooks",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jellybooks/web.git"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Jellybooks/web/issues"
  },
  "homepage": "https://github.com/Jellybooks/web",
  "keywords": [
    "jellybooks"
  ],
  "main": "dist/index.js",
  "typings": "types/index.d.ts",
  "files": [
    "dist",
    "src",
    "types"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "lint:fix": "tsdx lint --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "test:debug": "DEBUG=true node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/index.js",
  "size-limit": [
    {
      "path": "dist/shared-test.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/shared-test.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/preset-typescript": "^7.14.5",
    "@size-limit/preset-small-lib": "^4.11.0",
    "@types/jest": "^25.2.3",
    "husky": "^6.0.0",
    "size-limit": "^4.11.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^3.9.9"
  }
}
